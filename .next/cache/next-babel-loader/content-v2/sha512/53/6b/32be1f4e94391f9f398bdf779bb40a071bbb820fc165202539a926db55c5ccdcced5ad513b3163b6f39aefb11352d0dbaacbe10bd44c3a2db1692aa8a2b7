ation, thumb } = bike;\n  const [open, setOpen] = useState(false);\n  const controls = useAnimation();\n  const [ref, inView] = useInView({\n    threshold: 0.8,\n  });\n  useEffect(() => {\n    if (inView) {\n      controls.start('visible');\n    }\n  }, [controls, inView]);\n\n  return (\n    <motion.div\n      className={cn.container}\n      ref={ref}\n      animate={controls}\n      initial=\"hidden\"\n      transition={{ duration: 0.6, delay: 0.4, type: 'keyframes' }}\n      variants={{\n        visible: { opacity: 1, scale: 1 },\n        hidden: { opacity: 0, scale: 0 },\n      }}\n      onClick={() => setOpen(!open)}\n    >\n      <div key={id} className={cn.inner_container} style={{ display: open ? 'flex' : 'block' }}>\n        <motion.div\n          // initial={{ width: 300, height: 100 }}\n          animate={{ width: open ? 400 : 300, height: open ? 400 : 200 }}\n          className={cn.img_container}\n        >\n          <Image src={thumb} alt={title} layout=\"fill\" objectFit=\"cover\" />\n        </motion.div>\n\n        <motion.div className={cn.content}>\n          <h3>{title}</h3>\n          <span>Stolen @{stolen_location}</span>\n        </motion.div>\n      </div>\n    </motion.div>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}